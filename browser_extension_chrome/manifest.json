{
    "manifest_version": 3,
    "name": "Mouse Tracker 100ms click",
    "version": "3.4",
    "permissions": [
        "activeTab",
        "http://localhost:3000/"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]
}
